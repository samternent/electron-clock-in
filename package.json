{
  "name": "electron-quick-start",
  "productName": "Teamwork Clock In",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ 'Clock In' --overwrite --platform=darwin --arch=x64  --prune=true --out=release-builds",
    "package-all": "electron-packager ./ 'Clock In' --overwrite --all  --prune=true --out=release-builds",
    "sass": "node-sass src/styles/ -o public/assets/"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "babel-core": "^6.5.2",
    "electron-positioner": "^3.0.0",
    "extend": "^3.0.0",
    "menubar": "^5.2.0",
    "moment": "^2.14.1",
    "moment-duration-format": "^1.3.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^2.6.0",
    "reqwest": "^2.0.5",
    "tbg-flux-factory": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.23.0",
    "css-loader": "^0.23.1",
    "electron": "^1.4.3",
    "electron-debug": "^1.1.0",
    "electron-packager": "^8.5.2",
    "electron-prebuilt": "^1.4.13",
    "file-loader": "^0.8.5",
    "node-sass": "^3.8.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
